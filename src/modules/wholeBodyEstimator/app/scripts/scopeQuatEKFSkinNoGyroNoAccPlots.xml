        <graph remote="/QuaternionEKF/filteredOrientationEuler:o"
               index="1"
               color="Blue"
               title="Pitch quatEKF"
               size="3"
               type="lines" />
        <graph remote="/DirectFiltering/orientationEuler:o"
               index="1"
               color="Black"
               title=""
               size="3"
               type="lines" />
<!--       <graph remote="/DirectFiltering/tiltEuler:o"
              index="0"
              color="White"
              title=""
              size="3"
              type="lines" /> -->
    </plot>
    <plot gridx="1"
          gridy="0"
          hspan="1"
          vspan="1"
          title="Gyro raw measurements"
          size="60"
          minval="-5"
          maxval="5"
          bgcolor="Black">
        <graph remote="/QuaternionEKF/rawGyroMeas:o"
               index="0"
               color="Red"
               title=""
               size="3"
               type="lines" />
        <graph remote="/QuaternionEKF/rawGyroMeas:o"
               index="1"
               color="Green"
               title=""
               size="3"
               type="lines" />
    </plot>
    <plot gridx="1"
          gridy="1"
          hspan="1"
          vspan="1"
          title="Gyro raw measurements"
          size="60"
          minval="-5"
          maxval="5"
          bgcolor="Black">
        <graph remote="/QuaternionEKF/filteredOrientationEuler:o"
               index="2"
               color="Blue"
               title=""
               size="3"
               type="lines" />
